<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:toolbox="http://www.kwanza-systems.ru/schema/toolbox" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config></context:annotation-config>

    <bean id="dbtool.IEntityMappingRegistry" class="ru.kwanza.dbtool.orm.impl.mapping.EntityMappingRegistry"/>

    <bean id="dbtool.IEntityManager" class="ru.kwanza.dbtool.orm.impl.EntityManagerImpl"/>

    <bean id="dbtool.OperationFactory" class="ru.kwanza.dbtool.orm.impl.operation.OperationFactory"/>

    <bean id="dbtool.Fetcher" class="ru.kwanza.dbtool.orm.impl.fetcher.Fetcher"/>

    <bean id="dbtool.ProxyFactory" class="ru.kwanza.dbtool.orm.impl.fetcher.proxy.ProxyFactory"/>

    <bean id="dbtool.NonEntityMapping" class="ru.kwanza.dbtool.orm.impl.fetcher.NonEntityMapping"/>

</beans>