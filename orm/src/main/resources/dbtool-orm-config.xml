<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:toolbox="http://www.kwanza-systems.ru/schema/toolbox"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="dbtool.IEntityMappingRegistry" class="ru.kwanza.dbtool.orm.impl.mapping.EntityMappingRegistryImpl"/>

    <bean id="dbtool.IEntityManager" class="ru.kwanza.dbtool.orm.impl.EntityManagerImpl"/>

    <bean id="dbtool.Fetcher" class="ru.kwanza.dbtool.orm.impl.fetcher.Fetcher"/>

    <bean id="dbtool.ProxyFactory" class="ru.kwanza.dbtool.orm.impl.fetcher.proxy.ProxyFactory"/>

    <bean id="dbtool.NonEntityMapping" class="ru.kwanza.dbtool.orm.impl.fetcher.NonEntityMapping"/>

</beans>